{
  "name": "phosphor-webui",
  "description": "Web-based user interface for the OpenBMC firmware stack",
  "author": {
    "name": "OpenBMC Developers",
    "email": "openbmc@lists.ozlabs.org",
    "url": "https://www.openbmc.org/"
  },
